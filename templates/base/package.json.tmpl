{
  "name": "{{ .ProjectName }}",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "dependencies": {
    {{- if eq .CSS "tailwind" }}
    "@tailwindcss/vite": "^4.1.7",
    "tailwindcss": "^4.1.7",
    {{- end }}
    {{- if eq .CSS "bootstrap" }}
    "bootstrap": "^5.3.0",
    {{- end }}
    {{- if eq .Framework "vue" }}
    "vue": "^3.3.4",
    "vue-router": "^4.2.0"
    {{- else if eq .Framework "svelte" }}
    "svelte": "^3.58.0",
    "svelte-routing": "^1.6.0"
    {{- else if eq .Framework "react" }}
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^6.16.0"
    {{- end }}
  },
  "devDependencies": {
    "@eslint/js": "^9.26.0",
    {{- if eq .Framework "vue" }}
    "@vitejs/plugin-vue": "^4.4.1",
    "eslint-plugin-vue": "^9.8.0",
    {{- else if eq .Framework "svelte" }}
    "@sveltejs/vite-plugin-svelte": "^4.1.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "@tsconfig/svelte": "^4.0.0"
    {{- else if eq .Framework "react" }}
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react-refresh": "^1.3.0",
    "@vitejs/plugin-react": "^4.4.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    {{- end }}
    {{- if eq .Language "ts" }}
    "typescript-eslint": "^8.32.1",
    "typescript": "^5.8.3",
    {{- end }}
    "eslint": "^9.26.0",
    "globals": "^16.0.0",
    "vite": "^6.3.5"
  }
}
